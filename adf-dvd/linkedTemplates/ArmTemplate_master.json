{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-dvd"
		},
		"AzureBlobStorage_in_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_in'"
		},
		"PostgreSql_input_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'PostgreSql_input'"
		},
		"ls_cus_test_in_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_cus_test_in'"
		},
		"ls_forINPUTDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_forINPUTDB'"
		},
		"sql_DB_SOURCE_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sql_DB_SOURCE'"
		},
		"staging_server_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'staging_server'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-dvd_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_in_connectionString": {
						"value": "[parameters('AzureBlobStorage_in_connectionString')]"
					},
					"PostgreSql_input_connectionString": {
						"value": "[parameters('PostgreSql_input_connectionString')]"
					},
					"ls_cus_test_in_connectionString": {
						"value": "[parameters('ls_cus_test_in_connectionString')]"
					},
					"ls_forINPUTDB_connectionString": {
						"value": "[parameters('ls_forINPUTDB_connectionString')]"
					},
					"sql_DB_SOURCE_connectionString": {
						"value": "[parameters('sql_DB_SOURCE_connectionString')]"
					},
					"staging_server_connectionString": {
						"value": "[parameters('staging_server_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-dvd_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-dvd_ArmTemplate_0')]"
			]
		},
		{
			"name": "adf-dvd_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-dvd_ArmTemplate_1')]"
			]
		},
		{
			"name": "adf-dvd_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-dvd_ArmTemplate_2')]"
			]
		}
	]
}